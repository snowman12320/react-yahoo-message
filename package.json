    "husky": "^9.1.5",
    "lint-staged": "^15.2.9",
    "postcss": "^8.4.24",
    "prettier-plugin-tailwindcss": "0.2.5",
    "sass": "^1.77.8",
    "tailwindcss": "^3.3.2",
    "typescript": "^5.0.2"
